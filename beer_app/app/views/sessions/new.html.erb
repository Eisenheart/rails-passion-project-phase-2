<h1>Sessions#new</h1>
<p>Find me in app/views/session/new.html.erb</p>


<%= form_for :session , url: sessions_path, method: :post do |f| %>
  <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.label :password %><br>
    <%= f.text_field :password %>
  </p>


  <p>
    <%= f.submit "Log In" %>
  </p>
<% end %>
